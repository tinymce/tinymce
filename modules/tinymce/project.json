{
  "name": "tinymce",
  "tags": [ "open-source", "editor" ],
  "targets": {
    "build": {
      "inputs": [
        "tiny-product",
        "^tiny-product"
      ],
      "dependsOn": [
        "lint",
        "^build"
      ],
      "outputs": [
        "{projectRoot}/dist",
        "{projectRoot}/js",
        "{projectRoot}/lib"
      ]
    }
  }
}
