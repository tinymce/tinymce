    "tsconfig.json",
    "README.md",
    "LEGAL.txt",
    "LICENSE.txt"
  ],
  "main": "./lib/main/ts/main.js",
  "module": "./lib/main/ts/main.js",
  "types": "./lib/main/ts/main.d.ts"
