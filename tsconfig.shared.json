{
  "compilerOptions": {
    "moduleResolution": "node",
    "target": "es2018",
    "module": "es2015",
    "lib": ["es2020", "dom"],
    "importHelpers": true,
    "declaration": true,
    "declarationMap": true,
    "sourceMap": true,
    "composite": true,
    "outDir": "lib",
    "rootDir": ".",
    "baseUrl": ".",
    "forceConsistentCasingInFileNames": true,
    "noImplicitReturns": true,
    "noUnusedLocals": true,
    "strict": true,
    "types": []
  }
}
